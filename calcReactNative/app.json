{
  "expo": {
    "name": "Calculator React Native",
    "description": "Calculator app made by Sobhan Niroula on React Native",
    "slug": "calcReactNative",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "android": {
      "package": "sobhanniroula.reactnative",
      "permissions": [
        ""
      ]
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-blue.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}